{
	"_id": "hardwarezone",
	"url": "http://forums.hardwarezone.com.sg",
	"collectionName": "hardwarezone",
	"boardSelector": "tbody[id~=^collapseobj_forumbit_[0-9]+] a[href~=^/([a-z0-9]|-)+[0-9]+/$]",
	"thread": {
		"threadSelector": "tbody[id~=^threadbits_forum_[0-9]+$]>tr",
		"threadTitle": "a[id~=^thread_title_[0-9]+$]",
		"lastPostTime": "td:nth-child(4) > div",
		"lastPostUser": "td:nth-child(4) > div:first-child > a",
		"stickyClass": "hwz-sticky",
		"threadCreator": "td:nth-child(3) > div:last-child>span",
		"replies": "td:nth-child(5)",
		"views": "td:nth-child(6)"
	},
	"post": {
		"postSelector": "table[id~=^post[0-9]+$]",
		"postTime": "tr:first-child>td.thead:first-child",
		"postBody": "tr",
		"postContent": "div[id~=^post_message_[0-9]+$]",
		"postUser": "div[id~=^postmenu_[0-9]+$]",
		"postQuote": "div[id~=^post_message_[0-9]+$]>div.quote"
	},	
	"hasLast": false,
	"nextButtonSelector": ".pagination li.prevnext a:contains(Next)",
	"lastButtonSelector": "",
	"pageUrlPrefix": "-",
	"pageUrlSuffix": ".html",
	"boardPageUrlPrefix": "",
	"boardPageUrlSuffix": "",
	"useTimeAttribute": false,
	"timeAttributeName": "",
	"postOldToNew": true,
	"dateFormat": ""
}





{
	"_id": "renotalk",
	"url": "http://www.renotalk.com/forum/",
	"collectionName": "renotalk",
	"boardSelector": "li.cForumRow.ipsBox div.ipsDataItem_main>h4.ipsDataItem_title>a, li.cForumRow.ipsBox div.ipsDataItem_main>ul.ipsDataItem_subList>li>a",
	"thread": {
		"threadSelector": "ol.cTopicList>li.ipsDataItem",
		"threadTitle": "div.ipsDataItem_main>h4.ipsDataItem_title>a:not(.ipsTag_prefix)",
		"lastPostTime": "ul.ipsDataItem_lastPoster>li time",
		"lastPostUser": "ul.ipsDataItem_lastPoster>li:nth-child(2)",
		"stickyClass": "div.ipsDataItem_main>h4.ipsDataItem_title span[title=\"Pinned\"]",
		"threadCreator": "div.ipsDataItem_main>div.ipsDataItem_meta span[itemprop=\"name\"]",
		"replies": "ul.ipsDataItem_stats>li:first-child>span.ipsDataItem_stats_number",
		"views": "ul.ipsDataItem_stats>li.ipsType_light>span.ipsDataItem_stats_number"
	},
	"post": {
		"postSelector": "article.cPost.ipsComment",
		"postTime": "div.ipsComment_content>div.ipsComment_meta time",
		"postBody": "div.cPost_contentWrap",
		"postContent": "div.cPost_contentWrap>div.ipsType_normal",
		"postUser": ">aside.ipsComment_author>h3.cAuthorPane_author strong",
		"postQuote": "div.cPost_contentWrap>div.ipsType_normal blockquote"
	},
	"hasLast": true,
	"nextButtonSelector": "ul.ipsPagination:not(.ipsPagination_mini)>li.ipsPagination_next:not(.ipsPagination_inactive)>a",
	"lastButtonSelector": "ul.ipsPagination:not(.ipsPagination_mini)>li.ipsPagination_last>a",
	"threadPageUrlPrefix": "?page=",
	"threadPageUrlSuffix": "",
	"boardPageUrlPrefix": "?page=",
	"boardPageUrlSuffix": "",
	"useTimeAttribute": true,
	"timeAttributeName": "datetime",
	"postOldToNew": true,
	"dateFormat": "yyyy-MM-dd'T'HH:mm:ss'Z'"
}





{
	"_id": "vrzone",
	"url": "http://forums.vr-zone.com",
	"collectionName": "vrzone",
	"boardSelector": "#forums >li.forumbit_nopost li.forumbit_post >div.forumrow >div.foruminfo a:not([rel])",
	"thread": {
		"threadSelector": "#threads >li.threadbit, #stickies >li.threadbit",
		"threadTitle": "div.threadinfo h3.threadtitle >a.title",
		"lastPostTime": "dl.threadlastpost dd:last-child",
		"lastPostUser": "dl.threadlastpost >dd a.username",
		"stickyClass": ">div.sticky",
		"threadCreator": "div.threadinfo div.threadmeta a.username",
		"replies": "ul.threadstats >li:first-child >a",
		"views": "ul.threadstats >li:nth-child(2)"
	},
	"post": {
		"postSelector": "#posts >li.postcontainer",
		"postTime": "div.posthead span.postdate span.date",
		"postBody": "div.postdetails >div.postbody",
		"postContent": "div.postdetails >div.postbody div.postrow",
		"postUser": "div.postdetails >div.userinfo a.username",
		"postQuote": "div.postdetails >div.postbody div.postrow div.quote_container"
	},	
	"hasLast": true,
	"nextButtonSelector": "",
	"lastButtonSelector": "form.pagination >span.first_last>a",
	"threadPageUrlPrefix": "-",
	"threadPageUrlSuffix": ".html",
	"boardPageUrlPrefix": "index",
	"boardPageUrlSuffix": ".html",
	"useTimeAttribute": false,
	"timeAttributeName": "",
	"postOldToNew": true,
	"dateFormat": ""
}





